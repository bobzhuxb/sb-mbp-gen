<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bob.sm.mapper.SmMeetingMapper">

    <select id="joinSelectList" resultType="com.bob.sm.domain.SmMeeting">
        ${queryMain} ${ew.customSqlSegment}
    </select>

    <select id="joinSelectPage" resultType="com.bob.sm.domain.SmMeeting">
        ${queryMain} ${ew.customSqlSegment}
    </select>

    <select id="joinSelectCount" resultType="int">
        ${queryMain} ${ew.customSqlSegment}
    </select>

    <update id="organizerIdCascadeToNull">
        UPDATE sm_meeting SET organizer_id = null WHERE organizer_id = #{organizerId}
    </update>

    <update id="contractorIdCascadeToNull">
        UPDATE sm_meeting SET contractor_id = null WHERE contractor_id = #{contractorId}
    </update>

</mapper>