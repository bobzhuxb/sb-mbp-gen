relationship OneToOne {
	// 最新学校订单{最原始订单}
	SfstSchoolOrder{originalOrder} to SfstSchoolOrder{newestOrder}
}

relationship OneToMany {
	// 用户{角色}
	SystemUser{systemUserRole} to SystemUserRole{systemUser}
    // 角色{用户}
	SystemRole{systemUserRole} to SystemUserRole{systemRole}
    // 父操作许可{子操作许可}
	SystemPermission{child} to SystemPermission{parent}
	// 父资源{子资源}
	SystemResource{child} to SystemResource{parent}
    // 父组织机构{子组织机构}
	SystemOrganization{child} to SystemOrganization{parent}
    // 资源{许可}
	SystemResource{systemResourcePermission} to SystemResourcePermission{systemResource}
    // 许可{资源}
	SystemPermission{systemResourcePermission} to SystemResourcePermission{systemPermission}
    // 角色{资源}
	SystemRole{systemRoleResource} to SystemRoleResource{systemRole}
    // 资源{角色}
	SystemResource{systemRoleResource} to SystemRoleResource{systemResource}
    // 用户{资源}
	SystemUser{systemUserResource} to SystemUserResource{systemUser}
    // 资源{用户}
	SystemResource{systemUserResource} to SystemUserResource{systemResource}
    // 组织机构{用户}
    //!CASCADE_DELETE:NULL
	SystemOrganization{systemUser} to SystemUser{systemOrganization}
    // 父数据字典{子数据字典}
	SystemDictionary{child} to SystemDictionary{parent}
    // 供应商{上游供应商}
	SfstSupplier{upSfstSupplier} to SfstSupplier{downSfstSupplier}
    // 供应商{企业证照}
	SfstSupplier{sfstSupplierLicense} to SfstSupplierLicense{sfstSupplier}
    // 学校{学校类型}
	SfstSchool{sfstSchoolType} to SfstSchoolType{sfstSchool}
    // 学校{获奖证书}
	SfstSchool{sfstSchoolCertificate} to SfstSchoolCertificate{sfstSchool}
	// 所属学校{用户}
    //!CASCADE_DELETE:FORBIDDEN
	SfstSchool{systemUser} to SystemUser{sfstSchool}
	// 所属供应商{用户}
    //!CASCADE_DELETE:FORBIDDEN
	SfstSupplier{systemUser} to SystemUser{sfstSupplier}
    // 学校{供应商}
	SfstSchool{sfstSchoolSupplier} to SfstSchoolSupplier{sfstSchool}
    // 供应商{学校}
	SfstSupplier{sfstSchoolSupplier} to SfstSchoolSupplier{sfstSupplier}
    // 原料{原料图片}
	SfstMaterial{sfstMaterialPic} to SfstMaterialPic{sfstMaterial}
    // 原料{每日单价}
	SfstMaterial{sfstMaterialPrice} to SfstMaterialPrice{sfstMaterial}
    // 供应商{原料}
	SfstSupplier{sfstMaterial} to SfstMaterial{sfstSupplier}
    // 学校{学校原料限制}
	SfstSchool{sfstSchoolMaterialAttr} to SfstSchoolMaterialAttr{sfstSchool}
    // 原料{学校原料限制}
